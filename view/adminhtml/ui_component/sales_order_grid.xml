<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="mass_custom_action">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">mass_custom_action</item>
                        <item name="label" xsi:type="string" translate="true">Cyprus Post Shipping Label</item>
                        <item name="url" xsi:type="url" path="example/MassAction/Customaction"/>
                    </item>
                </argument>
            </action>
            <action name="mass_custom_action_com2go">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">mass_custom_action_com2go</item>
                        <item name="label" xsi:type="string" translate="true">Com2go Shipping Label</item>
                        <item name="url" xsi:type="url" path="example/MassAction/Com2golabel"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>